<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
				<sheetTitle>Institution</sheetTitle>
                <type>array</type>
                <el>
					<settings.art>
						<exclude>1</exclude>
						<label>Art der Suche</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items>
								<numIndex index="0">
									<numIndex index="0">nach Name</numIndex>
									<numIndex index="1">1</numIndex>
								</numIndex>
								<numIndex index="1">
									<numIndex index="0">nach Ausbildungsrichtung</numIndex>
									<numIndex index="1">2</numIndex>
								</numIndex>
								<numIndex index="2">
									<numIndex index="0">nach Sprache</numIndex>
									<numIndex index="1">3</numIndex>
								</numIndex>
								<numIndex index="3">
									<numIndex index="0">nach Vorklasse/Vorkurs</numIndex>
									<numIndex index="1">4</numIndex>
								</numIndex>
								<numIndex index="4">
									<numIndex index="0">Umkreissuche</numIndex>
									<numIndex index="1">5</numIndex>
								</numIndex>									
							</items>
						</config>
					</settings.art>					
					<settings.ansicht>
						<exclude>1</exclude>
						<label>Ansicht</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items>
								<numIndex index="0">
									<numIndex index="0">Liste</numIndex>
									<numIndex index="1">1</numIndex>
								</numIndex>
								<numIndex index="1">
									<numIndex index="0">Karte</numIndex>
									<numIndex index="1">2</numIndex>
								</numIndex>
								<numIndex index="2">
									<numIndex index="0">Liste + Karte</numIndex>
									<numIndex index="1">3</numIndex>
								</numIndex>
							</items>
						</config>
					</settings.ansicht>
					<settings.startingpoint>
							<exclude>1</exclude>
							<label>Datensatzsammlung</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>3</size>
								<maxitems>50</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
					</settings.startingpoint>
