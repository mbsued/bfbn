<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
				<sheetTitle>Meldung</sheetTitle>
                <type>array</type>
                <el>
					<settings.termin>
						<exclude>1</exclude>					
						<label>Termin</label>
						<config>
							<type>input</type>
							<renderType>inputDateTime</renderType>							
							<eval>datetime</eval>							
						</config>  
					</settings.termin>
					<settings.emailn>
						<exclude>1</exclude>					
						<label>Empfänger E-Mail (Nordbayern)</label>
						<config>
							<type>input</type>							
							<eval>email</eval>							
						</config>						
					</settings.emailn>
					<settings.emailo>
						<exclude>1</exclude>					
						<label>Empfänger E-Mail (Ostbayern)</label>
						<config>
							<type>input</type>							
							<eval>email</eval>							
						</config>
					</settings.emailo>
					<settings.emails>
						<exclude>1</exclude>					
						<label>Empfänger E-Mail (Südbayern)</label>
						<config>
							<type>input</type>							
							<eval>email</eval>							
						</config>
					</settings.emails>
					<settings.emailw>
						<exclude>1</exclude>					
						<label>Empfänger E-Mail (Westbayern)</label>
						<config>
							<type>input</type>							
							<eval>email</eval>							
						</config>
					</settings.emailw>					
					<settings.art>
						<exclude>1</exclude>
						<label>Art der Meldung</label>
						<config>
							<type>select</type>
                            <renderType>selectSingle</renderType>
                            <autoSizeMax>50</autoSizeMax>
                            <foreign_table>tx_bfbn_domain_model_meldungart</foreign_table>
                            <maxitems>1</maxitems>
							<minitems>1</minitems>
                            <size>1</size> 
						</config>
                    </settings.art>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>