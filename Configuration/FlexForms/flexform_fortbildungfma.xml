<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
				<sheetTitle>Fortbildung FMA</sheetTitle>
                <type>array</type>
                <el>
					<settings.startingpoint>
						<exclude>1</exclude>
						<label>Datensatzsammlung Fortbildung</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>3</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							<suggestOptions>
								<default>
									<addWhere>AND pages.doktype = 256</addWhere>
									<pidList>388</pidList>
									<pidDepth>1</pidDepth> 
								</default>
							</suggestOptions>
						</config>
					</settings.startingpoint>
					<settings.bezirke>
						<exclude>1</exclude>
						<label>MB-Bezirke</label>
						<config>
							<type>select</type>
							<renderType>selectMultipleSideBySide</renderType>
							<autoSizeMax>50</autoSizeMax>
							<foreign_table>tx_bfbn_domain_model_mbbezirk</foreign_table>
							<maxitems>4</maxitems>
							<size>10</size> 
						</config>
                    </settings.bezirke>
					<settings.pdftemplate>
						<exclude>1</exclude>
						<label>PDF-Template</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>						
							<foreign_table>tx_bfbn_domain_model_pdftemplate</foreign_table>
							<default>1</default>
						</config>
                    </settings.pdftemplate>
					<settings.htmltemplate>
						<exclude>1</exclude>
						<label>HTML-Template</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType> 
							<foreign_table>tx_bfbn_domain_model_htmltemplate</foreign_table>
							<default>1</default>							
						</config>
                    </settings.htmltemplate>
					<settings.ablageordner>
						<exclude>1</exclude>
                        <label>Ablageordner PDF</label>
                        <config>
							<type>folder</type>
							<elementBrowserEntryPoints>
								<default>2:/</default>								
							</elementBrowserEntryPoints>
                        </config>
                    </settings.ablageordner>					
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>