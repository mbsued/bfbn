<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
				<sheetTitle>Personen</sheetTitle>
                <type>array</type>
                <el>
					<settings.art>
						<exclude>1</exclude>
						<label>Art der Suche</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items>
								<numIndex index="0">
									<numIndex index="0">nach Institution</numIndex>
									<numIndex index="1">1</numIndex>
								</numIndex>
								<numIndex index="1">
									<numIndex index="0">nach Funktionen</numIndex>
									<numIndex index="1">2</numIndex>
								</numIndex>
								<numIndex index="2">
									<numIndex index="0">nach Institution und Funktionen</numIndex>
									<numIndex index="1">3</numIndex>
								</numIndex>									
							</items>
						</config>
					</settings.art>
					<settings.bezirke>
						<exclude>1</exclude>
						<label>MB-Bezirke</label>
						<config>
							<type>select</type>
							<renderType>selectMultipleSideBySide</renderType>
							<autoSizeMax>50</autoSizeMax>
							<foreign_table>tx_bfbn_domain_model_mbbezirk</foreign_table>
							<maxitems>4</maxitems>
							<size>10</size> 
						</config>
                    </settings.bezirke>
					<settings.funktionart>
						<exclude>1</exclude>
						<label>Art der Funktionen</label>
						<config>
							<type>select</type>
							<renderType>selectMultipleSideBySide</renderType>
							<autoSizeMax>50</autoSizeMax>
							<foreign_table>tx_bfbn_domain_model_funktionart</foreign_table>
							<maxitems>10</maxitems>
							<size>10</size> 
						</config>
                    </settings.funktionart>	
					<settings.startingpoint>
						<exclude>1</exclude>
						<label>Datensatzsammlung Personen</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>3</size>
							<maxitems>50</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							<wizards>
								<suggest>
									<type>suggest</type>
								</suggest>
							</wizards>
						</config>
					</settings.startingpoint>
					<settings.schulen>
						<exclude>1</exclude>
						<label>Datensatzsammlung Schulen</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>3</size>
							<maxitems>50</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							<wizards>
								<suggest>
									<type>suggest</type>
								</suggest>
							</wizards>
						</config>
					</settings.schulen>					
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>