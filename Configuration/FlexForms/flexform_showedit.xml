<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
				<sheetTitle>Institution</sheetTitle>
                <type>array</type>
                <el>
					<settings.startingpoint>
						<exclude>1</exclude>
						<label>Datensatzsammlung</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>3</size>
							<maxitems>50</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							<wizards>
							<suggest>
								<type>suggest</type>
							</suggest>
							</wizards>
						</config>
					</settings.startingpoint>				
                    <settings.institution>
						<exclude>1</exclude>
						<label>Institution</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<autoSizeMax>50</autoSizeMax>
							<foreign_table>tx_bfbn_domain_model_institution</foreign_table>
							<foreign_table_where> AND tx_bfbn_domain_model_institution.art != 1 ORDER BY tx_bfbn_domain_model_institution.mbbezirk ASC</foreign_table_where>
							<size>10</size>
						</config>
                    </settings.institution>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>